<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/charts">

    <fragment
        android:id="@+id/charts"
        android:name="com.brcxzam.sedapp.charts.Charts"
        android:label="fragment_charts"
        tools:layout="@layout/fragment_charts" />
    <fragment
        android:id="@+id/places"
        android:name="com.brcxzam.sedapp.places_ue.PlacesUE"
        android:label="fragment_places_ue"
        tools:layout="@layout/fragment_places_ue" />
    <fragment
        android:id="@+id/evaluations"
        android:name="com.brcxzam.sedapp.evaluation_ue.EvaluationUE"
        android:label="fragment_evaluation_ue"
        tools:layout="@layout/fragment_evaluation_ue" />
    <fragment
        android:id="@+id/evaluation"
        android:name="com.brcxzam.sedapp.evaluation_ue.QuestionsUE"
        android:label="fragment_questions_ue"
        tools:layout="@layout/fragment_questions_ue" />

    <action
        android:id="@+id/action_global_charts"
        app:destination="@id/charts" />
    <action
        android:id="@+id/action_global_evaluations"
        app:destination="@id/evaluations" />
    <action
        android:id="@+id/action_global_places"
        app:destination="@id/places" />
    <action
        android:id="@+id/action_global_evaluation"
        app:destination="@id/evaluation" />

</navigation>